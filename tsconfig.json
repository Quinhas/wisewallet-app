{
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"baseUrl": "./src",
		"paths": {
			"@components/*": [
				"/components/*"
			],
			"@pages/*": [
				"/pages/*"
			],
			"@services/*": [
				"/services/*"
			],
			"@utils/*": [
				"/utils/*"
			],
			"@contexts/*": [
				"/contexts/*"
			],
			"@hooks/*": [
				"/hooks/*"
			]
		},
		"typeRoots": [
			"./node_modules/@types",
			"./src/@types",
			"./src/models"
		],
		"allowJs": false,
		"skipLibCheck": true,
		"esModuleInterop": false,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx"
	},
	"include": [
		"src"
	],
	"references": [
		{
			"path": "./tsconfig.node.json"
		}
	]
}
